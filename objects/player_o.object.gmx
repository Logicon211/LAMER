if(health2 &lt;= 0 or y&gt;=room_height-200)
{
    if(health2&lt;0){health2=0}
    splodeColor = choose(c_red,c_yellow,c_orange)
    for(i=0;i&lt;20;i++)
    {
        effect_create_above(ef_smoke,x+random_range(-50,50),y+random_range(-70,70),300,c_orange);
        effect_create_above(ef_flare,x+random_range(-50,50),y+random_range(-70,70),300,c_orange);
    }
    splodeColor = choose(getPlayerColor(playerNum,0),getPlayerColor(playerNum,1),getPlayerColor(playerNum,2))
    for(i=0;i&lt;20;i++)
    {
        //effect_create_above(ef_firework,x+random_range(-30,30),y+random_range(-50,50),100,splodeColor);
    }
    respawn = instance_create(x,y,renewRoom);
    respawn.pIn = id;
    playerType = 2;

    x = 0;
    y = 0;
}

/***************************************************
