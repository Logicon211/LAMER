if(state=string('slideL'))
{
    with(shield_obj){instance_destroy()}
    //x -= 300*boost_power/weight;
    hspeed = -50;
    state=string('tonorm'){}
    
}
else if(state=string('slideR'))
{
    with(shield_obj){instance_destroy()}
    //x += 300*boost_power/weight;
    hspeed = 50;
    state=string('tonorm'){}
    
}
else
{
    //shield pressed
    if(controlSet[3])
    {
        core.gun_aim = 0;
        shield_counter += 1;
        if(state!=string('shield'))
        {
            shield_obj = instance_create(x,y,shield_o);
            shield_obj.player = id;
            state = string('shield');
        }
        target_hspeed = 0;
        shield_obj.x = core.x;
        shield_obj.y = core.y;
        if(shield_counter&gt;=1)
        {
            if(controlSet[6] and !controlSetPrv[6])
            {
                state=string('slideL');
                shield_counter = -10;
            }
            if(controlSet[7] and !controlSetPrv[7])
            {
                state=string('slideR');
                shield_counter = -10;
            }
        }
     
    }
    
    //shield released
    else if(!controlSet[3] and controlSetPrv[3])
    {
        effect_create_above(ef_flare,x,y,100,c_white)
        core.gun_aim = 0;
        with(shield_obj){instance_destroy()}
        state = string('tonorm');
        shield_counter = 0;
    }
    //down pressed
    /*
    else if(controlSet[5] and !airborne)
    {
        if(state!=string('kneel'))
        {
            state = string('kneel');
        }
        target_hspeed = 0;
        core.gun_aim = -1; // ** x&lt;0=down ** x=0=off ** x&gt;0=up
        if(controlSet[0])
        {
            attack[1] = true;
        }
        if(controlSet[1])
        {
            attack[2] = true;
        }
    }
    */
    else if(state=string('kneel'))
    {
        state=string('tonorm');
    }
    else if(state=string('Attack'))
    {
        target_hspeed = 0;
    }
    else
    {
        core.gun_aim = 0;
        //left and right keys pressed
        if(controlSet[6] &amp;&amp; controlSet[7])
        {
            target_hspeed = 0;
            walking=false;
        }
        //left key pressed
        else if(controlSet[6])
        {
            walking=true;
            if(boosting)
            {
                boost_move_x(-2);
            }
            else
            {
                boost_move_x(-1);
            }
        }
        //right key pressed
        else if(controlSet[7])
        {
            walking=true;
            if(boosting)
            {
                boost_move_x(2);
            }
            else
            {
                boost_move_x(1);
            }
        }
        //!left and !right pressed
        else
        {
            walking=false;
            target_hspeed = 0;
        }
        
        //jump pressed
        if(controlSet[2] or keyboard_check(ord('U')))
        {
            boostCounter = 0;
            y -= 3;
            vspeed -= 12;
        }
        
        //boost pressed
        if(controlSet[8])
        {
            boosting=true;
            if(airborne)
            {   vspeed -= 2
                if(controlSet[4])
                {   vspeed -=2;
                }
                if(controlSet[5])
                {   vspeed +=2;
                }
            }
            else
            {   boostCounter = 5;
            }
        }
        else
        {
            boosting=false;
            boostCounter -= 1;
        }
        
        if(controlSet[0])
        {
            attack[1] = true;
        }
        if(controlSet[1])
        {
            attack[2] = true;
        }
    }
}
