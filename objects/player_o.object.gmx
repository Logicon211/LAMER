  Image Transform
 ***************************************************/

if(hspeed!=0)
{
    image_xscale = hspeed/abs(hspeed);
    if(!boosting){core.image_xscale = image_xscale;}
}

/***************************************************
  END Image Transform
 ***************************************************/
 
/***************************************************
  State Controll
 ***************************************************/

if(state=string('tonorm'))
{
    counter += 1;
    if(counter&gt;=30)
    {
        state=string('normal');
        counter = 0;
    }
}

/***************************************************
  END State Controll
 ***************************************************/
 
//BoosterSize
boosterSize = 1 + target_vspeed/10 + weight/1000;
 
