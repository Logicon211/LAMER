{
    if(jumpCounter&lt;5)
    {
        jumpCounter += 1;
        core.x = x;
        core.y = y_ease(core.y,y+20-heightAdjust,4);
    }
    else if(jumpCounter=5)
    {
        jumpCounter = 100;
        core.x = x;
        core.y = y_ease(core.y,y+20-heightAdjust,4);
        vspeed -= 28;
        airborne = true;
        state = string('tonorm');
    }
}

/***************************************************
  END Jump State
 ***************************************************/

/***************************************************
  Normal State
 ***************************************************/
 
else if(state = string('normal'))
{
    core.x = x;
    core.y = y-heightAdjust;
    core.image_angle = angle_ease(core.image_angle,-hspeed*1.5,8);
}

/***************************************************
  END Normal State
 ***************************************************/

/***************************************************
  To Normal State
 ***************************************************/

else if(state = string('tonorm'))
{
    core.x = x_ease(core.x,x,4);
    core.y = y_ease(core.y,y-heightAdjust,4);
    core.image_angle = angle_ease(core.image_angle,-hspeed*1.5,4);

}

/***************************************************
  END To Normal State
 ***************************************************/

/***************************************************
  Shield State
 ***************************************************/

else if(state = string('shield'))
{
    core.x = x_ease(core.x,x+35*core.image_xscale,8);
    core.y = y_ease(core.y,y+40-heightAdjust,8);
    core.image_angle = angle_ease(core.image_angle,-25*core.image_xscale,4);
}

/***************************************************
  END Shield State
 ***************************************************/

/***************************************************
  Kneel State
 ***************************************************/

else if(state = string('kneel'))
{
    core.x = x_ease(core.x,x+35*core.image_xscale,8);
    core.y = y_ease(core.y,y+40-heightAdjust,8);
    core.image_angle = angle_ease(core.image_angle,-15*core.image_xscale,4);
}

/***************************************************
  END Kneel State
 ***************************************************/

/***************************************************
  Slide Left State
 ***************************************************/

else if(state = string('slideL'))
{
    core.x = x;
    if(core.image_xscale = 1)
    {
        core.y = y_ease(core.y,y-35-heightAdjust,4);
    }
    else
    {
        core.y = y-20-heightAdjust;
    }
    core.image_xscale = 1;    
    core.image_angle = angle_ease(core.image_angle,hspeed,4);
    
}

/***************************************************
  END Slide Left State
 ***************************************************/

/***************************************************
  Slide Right State
 ***************************************************/

else if(state = string('slideR'))
{
    core.x = x;
    if(core.image_xscale = -1)
    {
        core.y = y_ease(core.y,y-35-heightAdjust,4);
    }
    else
    {
        core.y = y-20-heightAdjust;
    }
    core.image_xscale = -1;
    core.image_angle = angle_ease(core.image_angle,hspeed,4);
    
}

/***************************************************
  END Slide Right State
 ***************************************************/

/***************************************************
  Land State
 ***************************************************/

else if(state = string('land'))
{
    if(landCounter&gt;0)
    {
        landCounter -= 1;
        core.x = x;
        core.y = y_ease(core.y,y+(landCounter*6)-heightAdjust,2);
    }
    else
    {
        state = string('tonorm');
    }
}

/***************************************************
  END Land State
 ***************************************************/
 
}
