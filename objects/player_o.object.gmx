    effect_create_above(ef_firework,x,y,30,c_yellow);
    effect_create_above(ef_firework,x,y,30,c_red);
    effect_create_above(ef_firework,x,y,30,c_orange);
    instance_destroy()
}

//***************************************************
//room speed test
if(gamepad_button_check(0,gp_shoulderrb))
{
    room_speed = 15;
}
else
{
    room_speed = 30;
}
//***************************************************

//quit game
if(quit_check())
{
    game_end();
}
//END quit game
//restart game
if(restart_check())
{
    game_restart();
}
//END restart game

/***************************************************
  Height Adjust and Collision Box
 ***************************************************/

image_yscale = height/32;

/***************************************************
