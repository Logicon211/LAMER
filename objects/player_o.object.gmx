 ***************************************************/


/***************************************************
  Controls
 ***************************************************/
if(state=string('slideL'))
{
    with(shield_obj){instance_destroy()}
    //x -= 300*boost_power/weight;
    hspeed = -50;
    state=string('tonorm'){}
    
}
else if(state=string('slideR'))
{
    with(shield_obj){instance_destroy()}
    //x += 300*boost_power/weight;
    hspeed = 50;
    state=string('tonorm'){}
    
}
else
{
    //shield pressed
    if(shield_check(playerNum))
    {
        core.gun_aim = 0;
        shield_counter += 1;
        if(state!=string('shield'))
        {
            shield_obj = instance_create(x,y,shield_o);
            state = string('shield');
        }
        target_hspeed = 0;
        shield_obj.x = core.x;
        shield_obj.y = core.y;
        if(shield_counter&gt;=1)
        {
            if(left_check_pressed(playerNum))
            {
                state=string('slideL');
                shield_counter = -10;
            }
            if(right_check_pressed(playerNum))
            {
                state=string('slideR');
                shield_counter = -10;
            }
        }
     
    }
    //shield released
    else if(shield_check_released(playerNum))
    {
        core.gun_aim = 0;
        with(shield_obj){instance_destroy()}
        state = string('tonorm');
        shield_counter = 0;
    }
    //down pressed
    else if(down_check(playerNum))
    {
        if(airborne)
        {
        }
        if(state!=string('kneel'))
        {
            state = string('kneel');
        }
        target_hspeed = 0;
        core.gun_aim = -1; // ** x&lt;0=down ** x=0=off ** x&gt;0=up
        if(attack1_check(playerNum))
        {
            attack[1] = true;
        }
        if(attack2_check(playerNum))
        {
            attack[2] = true;
        }
    }
    else if(state=string('kneel'))
    {
        state=string('tonorm');
    }
    else if(state=string('Attack'))
    {
        target_hspeed = 0;
    }
    else
    {
        core.gun_aim = 0;
        //left and right keys pressed
        if(left_check(playerNum) &amp;&amp; right_check(playerNum))
        {
            target_hspeed = 0;
            walking=false;
        }
        //left key pressed
        else if(left_check(playerNum))
        {
            walking=true;
            if(boosting)
            {
                boost_move_x(-2);
            }
            else
            {
                boost_move_x(-1);
            }
        }
        //right key pressed
        else if(right_check(playerNum))
        {
            walking=true;
            if(boosting)
            {
                boost_move_x(2);
            }
            else
            {
                boost_move_x(1);
            }
        }
        //!left and !right pressed
        else
        {
            walking=false;
            target_hspeed = 0;
        }
        
        //jump pressed
        if(jump_check(playerNum))
        {
            if(!airborne)
            {
                if(!boosting)
                {
                    state=string('jump');
                    jumpCounter = 0;
                    target_hspeed = 0;
                }
            }
            else{}
        }
        
        //boost pressed
        if(boost_check(playerNum))
        {
            boosting=true;
            if(jump_check(playerNum))
            {
                if(!airborne)
                {   y -= 3
                }
                airborne = true;
                vspeed -= 3;
            }
        }
        else
        {
            boosting=false;
        }
        
        if(attack1_check(playerNum))
        {
            attack[1] = true;
        }
        if(attack2_check(playerNum))
        {
            attack[2] = true;
        }
    }
}
//choose dominant attack
attackControl1(attack[1],attack[2],core.arm[1],core.arm[2]);
//state, attack1, attack2 are all set


