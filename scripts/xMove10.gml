armA.x += xInVar;
//lower arm
larmA.x += xInVar;
//arm2
armB.x += xInVar;
//lower arm2
larmB.x += xInVar;
//leg1
leg1.x += xInVar;
//lower leg1
lleg1.x += xInVar;
//leg2
leg2.x += xInVar;
//lower leg2
lleg2.x += xInVar;
