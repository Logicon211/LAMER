/*
    armA
    armB
    larmA
    larmB
    leg1
    leg2
    lleg1
    lleg2
    head
    core (self)
*/

xInVar = argument0;

//player
player.x += xInVar;
//core
x += xInVar;
//head
head.x += xInVar;
